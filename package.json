{
  "name": "tsx-to-svg-converter",
  "version": "1.0.0",
  "description": "Convert TSX icon files to clean SVG files",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "start": "tsx src/main.ts",
    "dev": "tsx --watch src/main.ts",
    "build": "tsc",
    "convert": "tsx src/main.ts"
  },
  "keywords": ["tsx", "svg", "converter", "icons"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

